{
	"name": "eleventy-base-blog",
	"version": "9.0.0",
	"description": "A starter repository for a blog web site using the Eleventy site generator.",
	"type": "module",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/blog-11ty/",
		"dev": "npx @11ty/eleventy --serve --quiet",
		"dev-ghpages": "npx @11ty/eleventy --pathprefix=/blog-11ty/ --serve --quiet",
		"debug": "cross-env DEBUG=Eleventy* npx @11ty/eleventy",
		"debug-dev": "cross-env DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "cross-env DEBUG=Eleventy:Benchmark* npx @11ty/eleventy"
	},
	"repository": {
		"type": "git",
		"url": "UPDATEME: git://github.com/11ty/eleventy-base-blog.git"
	},
	"author": {
		"name": "Viktor Zhurbin",
		"url": "UPDATEME: https://zachleat.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=22"
	},
	"bugs": {
		"url": "UPDATEME: https://github.com/11ty/eleventy-base-blog/issues"
	},
	"homepage": "UPDATEME: https://github.com/11ty/eleventy-base-blog#readme",
	"devDependencies": {
		"@11ty/eleventy": "^3.1.2",
		"@11ty/eleventy-img": "^6.0.4",
		"@11ty/eleventy-navigation": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^2.0.4",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
		"@types/react": "^19",
		"cross-env": "^7.0.3",
		"luxon": "^3.6.1",
		"prismjs": "^1.30.0",
		"zod": "^3.25.67",
		"zod-validation-error": "^3.5.2"
	},
	"dependencies": {
		"@zachleat/heading-anchors": "^1.0.3",
		"react-dom": "^19",
		"tsx": "^4.20.5"
	}
}
